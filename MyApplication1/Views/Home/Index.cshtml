@model IEnumerable<MyApplication1.Models.Student>
@using MyApplication1.Models;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<title>Students List</title>
<!DOCTYPE html>
<div style="font-family:Arial">
    <h2>Students List</h2>
<ul>
    @foreach (Student student in Model)
    {
       <li>
           @Html.ActionLink(student.FirstName,"Details",new {Id = student.ID })
       </li>
    }
</ul>
</div>
<p>

    @Html.ActionLink("Back to Departments", "Index","Department")
</p>
   

